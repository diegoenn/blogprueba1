<h1>
Cuarta pagina de Diego Narvaez - New
</h1>
<h2>
Nuevo Usuario
</h2>
<%= form_for(@usuarios) do |f| %>

<div class="field">
  <%= f.label :nombre %>
  <%= f.text_field :nombre %>
</div>
<div class="field">
  <%= f.label :email %>
  <%= f.text_field :email %>
</div>
<div class="actions">
  <%= f.submit 'Guardar' %>
</div>
<% end %>

<h3><%= link_to 'Atras', usuarios_path %></h3>
<h1>
Tercera pagina de Diego Narvaez - Edit
</h1>
<h2>
Editando Usuarios
</h2>
<%= form_for(@usuarios) do |f| %>

<div class="field">
  <%= f.label :nombre %><br>
  <%= f.text_field :nombre %>
</div>
<div class="field">
  <%= f.label :email %><br>
  <%= f.text_field :email %>
</div>
<div class="actions">
  <%= f.submit 'Guardar Cambios' %>
</div>
<% end %>

<h3><%= link_to 'Atras', usuarios_path %></h3>